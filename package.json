{
  "name": "sails-postgresql",
  "description": "a postgreSQL adapter for Waterline and Sails.js",
  "version": "0.12.2",
  "author": "Cody Stoltman <particlebanana@gmail.com>",
  "url": "http://github.com/balderdashy/sails-postgresql",
  "keywords": [
    "postgresql",
    "orm",
    "waterline",
    "sails"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/balderdashy/sails-postgresql.git"
  },
  "dependencies": {
    "pg": "~4.4.0",
    "lodash": "~3.10.0",
    "async": "~1.3.0",
    "waterline-errors": "~0.10.0",
    "waterline-sequel": "~0.5.1",
    "waterline-cursor": "~0.0.6"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "waterline-adapter-tests": "~0.10.15",
    "captains-log": "~0.11.1"
  },
  "scripts": {
    "test": "make test",
    "docker": "docker-compose run adapter bash"
  },
  "main": "lib/adapter",
  "directories": {
    "lib": "./lib"
  },
  "license": "MIT",
  "bugs": "https://github.com/balderdashy/sails-postgresql/issues",
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.0",
    "interfaces": [
      "semantic",
      "queryable",
      "migratable",
      "associations",
      "sql"
    ],
    "features": [
      "crossAdapter",
      "unique",
      "autoIncrement",
      "schemas"
    ]
  }
}
